{
    "version" : 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node",
            "functions": {
                "app/api/**/*": {
                "maxDuration": 30
                }
            }
        }
        
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
  }